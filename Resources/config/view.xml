<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="jsonannotation.view.listener.class">tbn\JsonAnnotationBundle\EventListener\JsonListener</parameter>
    </parameters>

    <services>
        <service id="jsonannotation.view.listener" class="%jsonannotation.view.listener.class%">
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
